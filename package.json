{
  "name": "iconified",
  "displayName": "Iconified",
  "description": "Set your own custom icons for files and folders",
  "publisher": "Alec269",
  "version": "1.0.0",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "url": "https://github.com/Alec269/Iconified-vscodeExt.git"
  },
  "bugs": {
    "url": "https://github.com/Alec269/Iconified-vscodeExt/issues"
  },
  "homepage": "https://github.com/Alec269/Iconified-vscodeExt#readme"
,
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "keywords": [
    "icons",
    "icon theme",
    "file icons",
    "folder icons",
    "custom icons",
    "theme"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "iconified.openFileIconsFolder",
        "title": "Iconified: Open File Icons Folder"
      },
      {
        "command": "iconified.openFolderIconsFolder",
        "title": "Iconified: Open Folder Icons Folder"
      },
      {
        "command": "iconified.refreshIcons",
        "title": "Iconified: Refresh Icons"
      }
    ],
    "iconThemes": [
      {
        "id": "iconified-custom-icons",
        "label": "Iconified",
        "path": "./icons/iconified-icon-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.0"
  }
}